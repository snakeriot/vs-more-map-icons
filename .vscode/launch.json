{
	"version": "1.0.0",
	"configurations": [
	{
		"name": "Launch Client (.NET)",
		"type": "clr",
		"request": "launch",
		"preLaunchTask": "Build (Debug)",
		"program": "${env:VINTAGE_STORY}/Vintagestory.exe",
		"args": [
			"--playStyle" , "creativebuilding",
			"--openWorld" , "runes test world",
			"--addModPath", "${workspaceFolder}/bin/Debug/net48",
			"--addOrigin" , "${workspaceFolder}/resources/assets"
		],
		"console": "internalConsole",
		"internalConsoleOptions": "openOnSessionStart",
	}]
}